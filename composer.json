{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.1",
        "awobaz/compoships": "^2.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "doctrine/dbal": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^10.0",
        "ovac/idoc": "^1.6",
        "queueworker/sansdaemon": "^1.2",
        "rap2hpoutre/fast-excel": "^3.1",
        "smalot/pdfparser": "^2.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.4",
        "swiftmailer/swiftmailer": "^6.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.13",
        "yajra/laravel-datatables-oracle": "^9.21",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.0",
        "sbamtr/laravel-source-encrypter": "^1.5",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helper.php",
            "app/Helpers/employee.php",
            "app/Helpers/merchHelper.php",
            "app/Helpers/PmsHelper.php",
            "app/Helpers/AccountsHelper.php",
            "app/Helpers/API.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
